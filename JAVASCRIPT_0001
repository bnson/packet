function checkRegExg(str_land, reg, content) 
{
	if (!content.match(reg) && content != "") {
		return "1";
	}
	return "0";
}

function trimSpace(str)
{
    return str.replace(/(?:(?:^|\n)\s+|\s+(?:$|\n))/g,"").replace(/\s+/g,"");
}


importPackage(javax.swing);
function checkBirthday(Vorname, Geburtsdatum){
	var regex = /^(((0[1-9]|[12]\d|3[01])(0[13578]|1[02])((19|[2-9]\d)\d{2}))|((0[1-9]|[12]\d|30)(0[13456789]|1[012])((19|[2-9]\d)\d{2}))|((0[1-9]|1\d|2[0-8])02((19|[2-9]\d)\d{2}))|(2902((1[6-9]|[2-9]\d)(0[48]|[2468][048]|[13579][26])|((16|[2468][048]|[3579][26])00))))$/
	var chk;
	var temp = Geburtsdatum.replace(/ /g,'');

	if(temp=="") return temp;

	if(temp.length==6){
		chk = checkDate6(temp);
	}

	else if(temp.length==8){
		chk = checkDate8(temp);
	}
	if(Vorname.indexOf(" u. ") != -1 || Vorname.indexOf(" und ") != -1) {
		return "";
	}
	if(regex.test(chk)==false){
		JOptionPane.showMessageDialog(null, "Nhập sai Birthday","Thông báo",JOptionPane.YES_NO_OPTION);
		return false;
	}

	var date = new Date();
	var dd = date.getDate();
	var mm = date.getMonth();
	var yy = date.getFullYear();
	var dd1 = chk.substring(0, 2);
	var mm1 = chk.substring(2, 4);
	var yy1 = chk.substring(4, 8);
	var curDate = new Date(yy, mm, dd);
	var inputDate = new Date(yy1,mm1-1,dd1);

	if(curDate<inputDate){
		JOptionPane.showMessageDialog(null, "Birthday không được lớn hơn ngày hiện tại","Thông báo",JOptionPane.YES_NO_OPTION);
		return false;
	}
	return chk;
}

//Function check 6 digits
function checkDate6(Day){
	var date = new Date();
	var yy = date.getFullYear() + '';
	yy = yy.substring(2,4)*1 + 1;
	var d = Day.substring(0, 2);
	var m = Day.substring(2, 4);
	var y = Day.substring(4, 6);

	if(y > yy) y = 19+y;
	else y = 20+y;

	return d+m+y;
}

//Function check 8 digits
function checkDate8(Day){
	var d = Day.substring(0, 2);
	var m = Day.substring(2, 4);
	var y = Day.substring(4, 8);

	return d+m+y;
}

importPackage(javax.swing);
function valid(Mobilfunknummer, Land_Lookup){

	var content = Mobilfunknummer;
	var v_land = Land_Lookup;

	var reg = /^[0-9]+$/;
	if (!content.match(reg) && content != "") {
		JOptionPane.showMessageDialog(null, "Only input number character!");
		return false;
	}
	var temp=content.substring(0,1);
	if(temp!="0"){
		return content;
	}
	
	//-------------------------------------------------------
	switch (v_land)
	{
		case "AF":
			var reg = /^(?:|0093)(?:|0)(?:7)(?:|[0-9])+$/;	
			var rs = checkRegExg(v_land, reg, content);	
			if (rs*1 != 1*1) 
			{
				JOptionPane.showMessageDialog(null,"Land " + v_land + " - The number isn't mobile!");
				return false;	
			}
			break;		
		case "AE":
			var reg = /^(?:|00971)(?:|0)(?:55|50)(?:|[0-9])+$/;	
			var rs = checkRegExg(v_land, reg, content);	
			if (rs*1 != 1*1) 
			{
				JOptionPane.showMessageDialog(null,"Land " + v_land + " - The number isn't mobile!");
				return false;	
			}
			break;		
		case "AG":
			var reg = /^(?:|001268)(?:|0)(?:7)(?:|[0-9])+$/;	
			var rs = checkRegExg(v_land, reg, content);	
			if (rs*1 != 1*1) 
			{
				JOptionPane.showMessageDialog(null,"Land " + v_land + " - The number isn't mobile!");
				return false;	
			}
			break;		
		case "AL":
			var reg = /^(?:|00355)(?:|0)(?:6)(?:|[0-9])+$/;	
			var rs = checkRegExg(v_land, reg, content);	
			if (rs*1 != 1*1) 
			{
				JOptionPane.showMessageDialog(null,"Land " + v_land + " - The number isn't mobile!");
				return false;	
			}
			break;		
		case "AM":
			var reg = /^(?:|00374)(?:|0)(?:9)(?:|[0-9])+$/;	
			var rs = checkRegExg(v_land, reg, content);	
			if (rs*1 != 1*1) 
			{
				JOptionPane.showMessageDialog(null,"Land " + v_land + " - The number isn't mobile!");
				return false;	
			}
			break;		
		case "AO":
			var reg = /^(?:|00244)(?:|0)(?:91|92)(?:|[0-9])+$/;	
			var rs = checkRegExg(v_land, reg, content);	
			if (rs*1 != 1*1) 
			{
				JOptionPane.showMessageDialog(null,"Land " + v_land + " - The number isn't mobile!");
