importPackage(javax.swing);
function valid(Vorname, Land_Lookup){

	var content = Vorname;
	var regex = /[А-Я]/gi;
	if (regex.test(content) && (Land_Lookup == "LT" || Land_Lookup == "LV")){
		JOptionPane.showMessageDialog(null,"Vorname - Khong duoc nhap ky tu tieng nga doi voi nuoc LT hoac LV!");
		return false;	
	}

	var myArr = ["'s","'t","aan","aan de","aan den","aan der","aan het","am","an de","an den","an der","auf","auf dem","auf den","auf der","auf'm","auf-m","aufm","aus","aus dem","aus den","aus der","aus'm","d'","d.","da","dagli","dai","dal","dall'","dalla","das","de","de la","de las","de le","de li","de los","de'","degli","dei","del","dell'","della","delle","dello","dem","den","der","des","di","do","dos","du","gli","het","im","in","in dem","in den","in der","in't","jun","l'","la","las","le","les","li","lo","los","op","op de","op den","op der","sen","ten","ter","thor","tor","tot","uit","uit den","v.","v. d","v. d.","v. den","v. der","v.d.","van","van de","van den","van der","van het","van't","vande","vanden","ver","vom","von","von de","von dem","von den","von der","von und zu","von und zu der","von und zum","von und zur","von zu","von zum","von zur","zu","zum","zur"];

	var content = Vorname;
	var rs = upperCaseNoneException(content);
	rs = rs.replace("'S ", "'s ");
	rs = rs.replace("'T ", "'t ");
	for (i=0;i<myArr.length;i++){
		rs = rs.replace(myArr[i].substring(0,1).toUpperCase()+myArr[i].substring(1,myArr[i].length)+" ", myArr[i]+" ");
	}
	rs = rs.replace(/ u. /gi, " u. ");
	rs = rs.replace(/ und /gi, " und ");
	return rs;
}

function upperCaseNoneException(str){
	var value = str.toLowerCase().replace(/\b\w/g, function(match){return match.toUpperCase();});
	value = value.replace(/(?:(?:^|\n)\s+|\s+(?:$|\n))/g,"").replace(/\s+/g," ");

	//var myArr = [" -","- "," - "," U. "];
	var myArr = [" -","- "," - "];
	for(i=0; i<myArr.length; i++)
	{
		//if(myArr[i] == " U. ") value = value.replace(myArr[i], " und ");
		//else 
		value = value.replace(myArr[i], "-");
	}
	return value;
}

importPackage(javax.swing);
function valid(Nachname, Land_Lookup){

	var content = Nachname;
	var regex = /[А-Я]/gi;
	if (regex.test(content) && (Land_Lookup == "LT" || Land_Lookup == "LV")){
		JOptionPane.showMessageDialog(null,"Nachname - Khong duoc nhap ky tu tieng nga doi voi nuoc LT hoac LV!");
		return false;	
	}

	var myArr = ["'s","'t","aan","aan de","aan den","aan der","aan het","am","an de","an den","an der","auf","auf dem","auf den","auf der","auf'm","auf-m","aufm","aus","aus dem","aus den","aus der","aus'm","d'","d.","da","dagli","dai","dal","dall'","dalla","das","de","de la","de las","de le","de li","de los","de'","degli","dei","del","dell'","della","delle","dello","dem","den","der","des","di","do","dos","du","gli","het","im","in","in dem","in den","in der","in't","jun","l'","la","las","le","les","li","lo","los","op","op de","op den","op der","sen","ten","ter","thor","tor","tot","uit","uit den","v.","v. d","v. d.","v. den","v. der","v.d.","van","van de","van den","van der","van het","van't","vande","vanden","ver","vom","von","von de","von dem","von den","von der","von und zu","von und zu der","von und zum","von und zur","von zu","von zum","von zur","zu","zum","zur"];

	var content = Nachname;
	var rs = upperCaseNoneException(content);
	rs = rs.replace("'S ", "'s ");
	rs = rs.replace("'T ", "'t ");
	for(i=0;i<myArr.length;i++){
		rs = rs.replace(myArr[i].substring(0,1).toUpperCase()+myArr[i].substring(1,myArr[i].length)+" ", myArr[i]+" ");
	}
	rs = rs.replace(/ u. /gi, " u. ");
	rs = rs.replace(/ und /gi, " und ");
	return rs;

}

function upperCaseNoneException(str){
	var value = str.toLowerCase().replace(/\b\w/g, function(match){return match.toUpperCase();});
	value = value.replace(/(?:(?:^|\n)\s+|\s+(?:$|\n))/g,"").replace(/\s+/g," ");

	//var myArr = [" -","- "," - "," U. "];
	var myArr = [" -","- "," - "];	
	for(i=0; i<myArr.length; i++)
	{
		//if(myArr[i] == " U. ") value = value.replace(myArr[i], " und ");
		//else value = value.replace(myArr[i], "-");
		value = value.replace(myArr[i], "-");
	}
	return value;
}

importPackage(javax.swing);
function valid(Anrede, Vorname, Anrede_Lookup){

	var content = Anrede.toLowerCase();
	var vname = Vorname.toLowerCase();

	if (content != "" && vname != "" && (vname.indexOf(" u. ") != -1 || vname.indexOf(" und ") != -1)) {
		return "";
	}

	if (content == "" && vname != "" && vname.indexOf(" u. ") == -1 && Anrede_Lookup != "") {
		content = Anrede_Lookup.toLowerCase();
	}

	var result = "";

	var i_str = content;
	i_str = remove_space(i_str);

	var pattern_anrede = /^(1|2|herr|frau)$/;

	if (i_str != "" && !pattern_anrede.test(i_str)) {
		JOptionPane.showMessageDialog(null, "Anrede - Gia tri nhap khong hop le, chi duoc nhap '1' hoac ' 2' hoac 'Herr' hoac 'Frau'!");
		return false;
	} else {
		result = i_str.replace("1","Frau").replace("2","Herr").replace("frau","Frau").replace("herr","Herr");
	}
	
	return result;

}

function remove_space(str) 
{
    return str.replace(/\s+/gi,"");
}

importPackage(javax.swing);
function valid(Titel, Land_Lookup){
	var content = Titel;

	var regex = /[А-Я]/gi;
	if (regex.test(content) && (Land_Lookup == "LT" || Land_Lookup == "LV")){
		JOptionPane.showMessageDialog(null,"Titel - Khong duoc nhap ky tu tieng nga doi voi nuoc LT hoac LV!");
		return false;	
	}

	return trimSpace(content);
}

function trimSpace(str) {
    return str.replace(/^\s*/, "").replace(/\s*$/, "");
}

importPackage(javax.swing);
function valid(Strasse, Land){
	var contents = Strasse;

	var regex = /[А-Я]/gi;
	if (regex.test(contents) && (Land == "LT" || Land == "LV")){
		JOptionPane.showMessageDialog(null,"Strasse - Khong duoc nhap ky tu tieng nga doi voi nuoc LT hoac LV!");
		return false;	
	}

	var rs = contents.substring(0,1).toUpperCase() + contents.substring(1,contents.length);
	return rs.replace(/(?:(?:^|\n)\s+|\s+(?:$|\n))/g,"").replace(/\s+/g," ");
}

function trimSpace(str) {
    return str.replace(/^\s*/, "").replace(/\s*$/, "");
}

importPackage(javax.swing);
function valid(PLZ, Land, Land_Lookup){
	var content = PLZ.toUpperCase();

	var regex = /[?-?]/gi;
	if (regex.test(content) && (Land_Lookup == "LT" || Land_Lookup == "LV")){
		JOptionPane.showMessageDialog(null,"PLZ - Khong duoc nhap ky tu tieng nga doi voi nuoc LT hoac LV!");
		return false;	
	}


	var regNum = /^[0-9]+$/;

	if (content == "") {
		return true;
	}
	if (Land == ""){
		if(/^(?:([d]{5}-[\d]{4})|([\d]{2}-[\d]{3})|([\d]{4,6})|([\d]{4}[ ][A-Z]{2})|([A-Z][\d][ ][\d][A-Z]{2})|([A-Z][\d]{2}[ ][\d][A-Z]{2})|([A-Z][\d][A-Z][ ][\d][A-Z]{2})|([A-Z]{2}[\d][ ][\d][A-Z]{2})|([A-Z]{2}[\d]{2}[ ][\d][A-Z]{2})|([A-Z]{2}[\d][A-Z][ ][\d][A-Z]{2})|([\d]{4}[\-][\d]{3})|([A-Z][\d][A-Z][ ][\d][A-Z][\d])|([A-Z][\d]{4}[A-Z]{3})|([A-Z]{3}[ ][\d]{2})|([A-Z]{2} \d{5})|([A-Z0-9]{2,4}[ ][A-Z0-9]{3})|([A-Z0-9]{3}[ ][A-Z0-9]{3})|(\d{2}-\d{3})|(\d{3}[ ]\d{2})|(\d{3}[ ]\d{3})|(\d{4}-\d{3}))$/.test(content)) return content;
		JOptionPane.showMessageDialog(null,"PLZ - Khong dung format cua XML");
 		return false;	
		
	}


	switch (Land) {
		case "DE":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land DE - PLZ only input number character!");
				return false;			
			}
			if (content.length != 5) {
				JOptionPane.showMessageDialog(null, "Land DE - PLZ must have 5 number character!");
				return false;
			}
			break;
		//------------------------------------------------------------------------------------------
		case "AT":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land AT - PLZ only input number character!");
				return false;			
			}
			if (content.length != 4) {
				JOptionPane.showMessageDialog(null, "Land AT - PLZ must have 4 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "BE":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land BE - PLZ only input number character!");
				return false;			
			}
			if (content.length != 4) {
				JOptionPane.showMessageDialog(null, "Land BE - PLZ must have 4 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "BG":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land BG - PLZ only input number character!");
				return false;			
			}
			if (content.length != 4) {
				JOptionPane.showMessageDialog(null, "Land BG - PLZ must have 4 number character!");
				return false;
			}
			break;		
		//----------------------------------------------------------------------------------------------
		case "CZ":
			//var regNL = /^[0-9][0-9][0-9][ ][0-9][0-9]$/;
			var regNL = /^[0-9][0-9][0-9][0-9][0-9]$/;
			if (!content.match(regNL)) {
				JOptionPane.showMessageDialog(null, "Land CZ - PLZ invalid!");
				return false;			
			}
			break;		
		//-------------------------------------------------------------------------------------------
		case "ES":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land ES - PLZ only input number character!");
				return false;			
			}
			if (content.length != 5) {
				JOptionPane.showMessageDialog(null, "Land ES - PLZ must have 5 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "FR":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land FR - PLZ only input number character!");
				return false;			
			}
			if (content.length != 5) {
				JOptionPane.showMessageDialog(null, "Land FR - PLZ must have 5 number character!");
				return false;
			}
			break;
		//---------------------------------------------------------------------------------------------
		case "GB":
			var regNL = /^\w{5,7}$/;
			if (!content.match(regNL)) {
				JOptionPane.showMessageDialog(null, "Land GB - PLZ invalid!");
				return false;			
			}
			break;			
		//-------------------------------------------------------------------------------------------
		case "HR":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land HR - PLZ only input number character!");
				return false;			
			}
			if (content.length != 5) {
				JOptionPane.showMessageDialog(null, "Land HR - PLZ must have 5 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "HU":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land HU - PLZ only input number character!");
				return false;			
			}
			if (content.length != 4) {
				JOptionPane.showMessageDialog(null, "Land HU - PLZ must have 4 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "LT":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land LT - PLZ only input number character!");
				return false;			
			}
			if (content.length != 5) {
				JOptionPane.showMessageDialog(null, "Land LT - PLZ must have 5 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "LV":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land LV - PLZ only input number character!");
				return false;			
			}
			if (content.length != 4) {
				JOptionPane.showMessageDialog(null, "Land LV - PLZ must have 4 number character!");
				return false;
			}
			break;
		//---------------------------------------------------------------------------------------------
		case "NL":
			//var regNL = /^[0-9][0-9][0-9][0-9][ ][a-zA-Z][a-zA-Z]$/;
			var regNL = /^[0-9][0-9][0-9][0-9][a-zA-Z][a-zA-Z]$/;
			if (!content.match(regNL)) {
				JOptionPane.showMessageDialog(null, "Land NL - PLZ invalid!");
				return false;			
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "PL":
			//var regNL = /^[0-9][0-9][-][0-9][0-9][0-9]$/;
			var regNL = /^[0-9][0-9][0-9][0-9][0-9]$/;
			if (!content.match(regNL)) {
				JOptionPane.showMessageDialog(null, "Land PL - PLZ invalid!");
				return false;			
			}
			break;			
		//----------------------------------------------------------------------------------------
		case "RO":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land RO - PLZ only input number character!");
				return false;			
			}
			if (content.length != 6) {
				JOptionPane.showMessageDialog(null, "Land RO - PLZ must have 6 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "RU":
			var regNL = /^\d{6}$/;
			if (!content.match(regNL)) {
				JOptionPane.showMessageDialog(null, "Land RU - PLZ invalid!");
				return false;			
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "SI":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land SI - PLZ only input number character!");
				return false;			
			}
			if (content.length != 4) {
				JOptionPane.showMessageDialog(null, "Land SI - PLZ must have 4 number character!");
				return false;
			}
			break;				
		//-------------------------------------------------------------------------------------------
		case "SK":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land SK - PLZ only input number character!");
				return false;			
			}
			if (content.length != 5) {
				JOptionPane.showMessageDialog(null, "Land SK - PLZ must have 5 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "CH":
			var regNL = /^\d{4}$/;
			if (!content.match(regNL)) {
				JOptionPane.showMessageDialog(null, "Land CH - PLZ invalid!");
				return false;			
			}
			break;		
		//-------------------------------------------------------------------------------------------
		case "ET":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land ET - PLZ only input number character!");
				return false;			
			}
			if (content.length != 5) {
				JOptionPane.showMessageDialog(null, "Land ET - PLZ must have 5 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "BA":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land BA - PLZ only input number character!");
				return false;			
			}
			if (content.length != 5) {
				JOptionPane.showMessageDialog(null, "Land BA - PLZ must have 5 number character!");
				return false;
			}
			break;			
		//-------------------------------------------------------------------------------------------
		case "CA":
			var regNL = /^\w{6}$/;
			if (!content.match(regNL)) {
				JOptionPane.showMessageDialog(null, "Land CA - PLZ invalid!");
				return false;			
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "DK":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land DK - PLZ only input number character!");
				return false;			
			}
			if (content.length != 4) {
				JOptionPane.showMessageDialog(null, "Land DK - PLZ must have 4 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "EE":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land EE - PLZ only input number character!");
				return false;			
			}
			if (content.length != 5) {
				JOptionPane.showMessageDialog(null, "Land EE - PLZ must have 5 number character!");
				return false;
			}
			break;	
		//-------------------------------------------------------------------------------------------
		case "FI":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land FI - PLZ only input number character!");
				return false;			
			}
			if (content.length != 5) {
				JOptionPane.showMessageDialog(null, "Land FI - PLZ must have 5 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "GR":
			var regNL = /^\d{5}$/;
			if (!content.match(regNL)) {
				JOptionPane.showMessageDialog(null, "Land GR - PLZ invalid!");
				return false;			
			}
			break;
		//-------------------------------------------------------------------------------------------
		//IE
		//-------------------------------------------------------------------------------------------
		//-------------------------------------------------------------------------------------------
		case "IL":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land IL - PLZ only input number character!");
				return false;			
			}
			if (content.length != 5) {
				JOptionPane.showMessageDialog(null, "Land IL - PLZ must have 5 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "IT":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land IT - PLZ only input number character!");
				return false;			
			}
			if (content.length != 5) {
				JOptionPane.showMessageDialog(null, "Land IT - PLZ must have 5 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------		
		case "LI":
			var regNL = /^\d{4}$/;
			if (!content.match(regNL)) {
				JOptionPane.showMessageDialog(null, "Land LI - PLZ invalid!");
				return false;			
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "LU":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land LU - PLZ only input number character!");
				return false;			
			}
			if (content.length != 4) {
				JOptionPane.showMessageDialog(null, "Land LU - PLZ must have 4 number character!");
				return false;
			}
			break;					
		//-------------------------------------------------------------------------------------------
		case "MK":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land MK - PLZ only input number character!");
				return false;			
			}
			if (content.length != 4) {
				JOptionPane.showMessageDialog(null, "Land MK - PLZ must have 4 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "MC":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land MC - PLZ only input number character!");
				return false;			
			}
			if (content.length != 5) {
				JOptionPane.showMessageDialog(null, "Land MC - PLZ must have 5 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "NO":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land NO - PLZ only input number character!");
				return false;			
			}
			if (content.length != 4) {
				JOptionPane.showMessageDialog(null, "Land NO - PLZ must have 4 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "PT":
			//var regNL = /^[0-9][0-9][0-9][ ][0-9][0-9][0-9]$/;
			var regNL = /^\d{7}$/;
			if (!content.match(regNL)) {
				JOptionPane.showMessageDialog(null, "Land PT - PLZ invalid!");
				return false;			
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "SE":
			var regNL = /^[0-9][0-9][0-9][0-9][0-9]$/;
			if (!content.match(regNL)) {
				JOptionPane.showMessageDialog(null, "Land SE - PLZ invalid!");
				return false;			
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "YU":
			var regNL = /^\d{6}$/;
			if (!content.match(regNL)) {
				JOptionPane.showMessageDialog(null, "Land YU - PLZ invalid!");
				return false;			
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "TR":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land TR - PLZ only input number character!");
				return false;			
			}
			if (content.length != 5) {
				JOptionPane.showMessageDialog(null, "Land TR - PLZ must have 5 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "UA":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land UA - PLZ only input number character!");
				return false;			
			}
			if (content.length != 5) {
				JOptionPane.showMessageDialog(null, "Land UA - PLZ must have 5 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		//USA
		//-------------------------------------------------------------------------------------------
		case "BY":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land BY - PLZ only input number character!");
				return false;			
			}
			if (content.length != 6) {
				JOptionPane.showMessageDialog(null, "Land BY - PLZ must have 5 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		case "CY":
			if (!content.match(regNum)) {
				JOptionPane.showMessageDialog(null, "Land CY - PLZ only input number character!");
				return false;			
			}
			if (content.length != 4) {
				JOptionPane.showMessageDialog(null, "Land CY - PLZ must have 4 number character!");
				return false;
			}
			break;
		//-------------------------------------------------------------------------------------------
		default:
			if(/^(?:([d]{5}-[\d]{4})|([\d]{2}-[\d]{3})|([\d]{4,6})|([\d]{4}[ ][A-Z]{2})|([A-Z][\d][ ][\d][A-Z]{2})|([A-Z][\d]{2}[ ][\d][A-Z]{2})|([A-Z][\d][A-Z][ ][\d][A-Z]{2})|([A-Z]{2}[\d][ ][\d][A-Z]{2})|([A-Z]{2}[\d]{2}[ ][\d][A-Z]{2})|([A-Z]{2}[\d][A-Z][ ][\d][A-Z]{2})|([\d]{4}[\-][\d]{3})|([A-Z][\d][A-Z][ ][\d][A-Z][\d])|([A-Z][\d]{4}[A-Z]{3})|([A-Z]{3}[ ][\d]{2})|([A-Z]{2} \d{5})|([A-Z0-9]{2,4}[ ][A-Z0-9]{3})|([A-Z0-9]{3}[ ][A-Z0-9]{3})|(\d{2}-\d{3})|(\d{3}[ ]\d{2})|(\d{3}[ ]\d{3})|(\d{4}-\d{3}))$/.test(content) || content == "") 	return trimSpace(content);
		JOptionPane.showMessageDialog(null,"PLZ - Khong dung format cua XML");
 		return false;
	}	

	return trimSpace(content);

}

function trimSpace(str) {
    return str.replace(/^\s*/, "").replace(/\s*$/, "");
}

importPackage(javax.swing);
function valid(ORT, Land){
	var content = ORT;

	var regex = /[А-Я]/gi;
	if (regex.test(content) && (Land == "LT" || Land == "LV")){
		JOptionPane.showMessageDialog(null,"ORT - Khong duoc nhap ky tu tieng nga doi voi nuoc LT hoac LV!");
		return false;	
	}

	var rs = content.substring(0,1).toUpperCase() + content.substring(1,content.length);
	return rs.replace(/(?:(?:^|\n)\s+|\s+(?:$|\n))/g,"").replace(/\s+/g," ");
	//return update_Ort(content);
}

function update_Ort(str)
{
     str = str.toLowerCase().replace( /\b((m)(a?c))?(\w)/g,
                         function($1, $2, $3, $4, $5) {
                              if($2){
                                   return $3.toUpperCase()+$4+$5.toUpperCase();
                              } 
                              return $1.toUpperCase(); 
                              });
     var contents = str.replace(/(?:(?:^|\n)\s+|\s+(?:$|\n))/g,"").replace(/\s+/g," ");
	//---------------------------------
	//var myArr = [" -","- "," - "];
	var myArr = [];
	for (var i = 0; i < myArr.length; i++)
	{
		var match = new RegExp(myArr[i], "ig");
		if (myArr[i] == " U. ") {	contents = contents.replace(match," u. "); }
		else { contents = contents.replace(match,"-"); }
	}
	return contents;
}

function valid(Land, Land_Lookup){
	var content = Land_Lookup;
	if (content == "Other") {
		return "";
	}
	return content;
}

importPackage(javax.swing);
function valid(Telefonnummer, Land_Lookup){

	var content = Telefonnummer;
	var v_land = Land_Lookup;
	var reg = /^[0-9]+$/;	
	if (!content.match(reg) && content != "") {
		JOptionPane.showMessageDialog(null, "Only input number character!");
		return false;
	}
	var temp=content.substring(0,1);
	if(temp!="0"){
		return content;
	}
