-- Table: address

-- DROP TABLE address;

CREATE TABLE address
(
  anrede character varying,
  vorname text,
  nachname text,
  company_name text,
  strasse character varying,
  hausenummer character varying,
  strasse_haus character varying(255),
  gleiort character varying,
  land character varying,
  plz character varying,
  ort character varying(255),
  note character varying,
  vorwahlen character varying,
  telefon character varying,
  vornwahl_and_telefon character varying,
  fax character varying,
  sub_ort character varying(50),
  titel character varying,
  plz_old character varying,
  ort_old character varying,
  strasse_old character varying,
  anrede_new character varying,
  vorname_old character varying(255),
  name_old character varying(255),
  zusatz character varying(255)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE address
  OWNER TO address;
GRANT ALL ON TABLE address TO address;
GRANT ALL ON TABLE address TO vnthang;
GRANT SELECT ON TABLE address TO read_pri;
GRANT SELECT, UPDATE, INSERT, DELETE ON TABLE address TO dml_pri;
GRANT SELECT ON TABLE address TO app_masterdata;

