            File f = new File(UIBBRaun.class.getProtectionDomain().getCodeSource().getLocation().getPath());
            currentPath = f.getPath();
            if (currentPath.lastIndexOf(".jar") != -1) {
                if (currentPath.contains("/")) {
                    currentPath = this.currentPath.substring(0, currentPath.lastIndexOf("/") + 1);
                }
                if (currentPath.contains("\\")) {
                    currentPath = this.currentPath.substring(0, currentPath.lastIndexOf("\\") + 1);
                }
            }
            System.out.println("-- OS System: " + System.getProperty("os.name"));
            System.out.println("-- OS Separator: " + System.getProperty("file.separator"));
            if ((this.currentPath.lastIndexOf("\\") == -1) && (currentPath.lastIndexOf("/") == -1)) {
                this.currentPath += System.getProperty("file.separator");
            }
            System.out.println("-- currentPath: " + currentPath);

